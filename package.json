{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "deploy": "rm -rf node_modules/.cache && npm run build && touch public/.nojekyll && git add public/build && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix public/ origin gh-pages"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "genie-hki": "^1.0.0",
    "rxjs": "^6.5.5",
    "sirv-cli": "^0.4.4"
  }
}
